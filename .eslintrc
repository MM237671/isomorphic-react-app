{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-no-bind": 0,
    "react/require-render-return": 0,
    "react/prefer-stateless-function": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "import/no-unresolved": 0
  },
  "plugins": [
    "react"
  ],
  "settings": {
    "import/resolve": {
      "extensions": [ ".es6", ".js", ".jsx" ]
    },
    "import/parser": "babel-eslint"
  }
}
